<h1>New Booking</h1>

<%= form_with(model: @booking, url: trainer_service_bookings_path(@trainer, @service)) do |form| %>

  <div>
    <%= form.label :status %>
    <%= form.number_field :status %>
  </div>

  <div>
    <%= form.label :start_time %>
    <%= form.datetime_select :start_time %>
  </div>

  <div>
    <%= form.label :end_time %>
    <%= form.datetime_select :end_time %>
  </div>

  <div>
    <%= form.submit "Create Booking" %>
  </div>
<% end %>
